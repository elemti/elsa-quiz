{
  "name": "elsa-quiz",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "db:generate": "npx dotenv-flow -- kysely-codegen --out-file ./db/db.d.ts",
    "db:migrate-latest": "pnpm db:migrate latest; pnpm db:generate",
    "db:migrate-make": "pnpm db:migrate make",
    "db:migrate": "npx dotenv-flow -- npx kysely migrate",
    "postinstall": "pnpm run db:generate",
