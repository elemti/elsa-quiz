    "@mui/material": "^6.1.10",
    "@mui/material-nextjs": "^6.1.9",
    "@t3-oss/env-nextjs": "^0.11.1",
    "kysely": "^0.27.4",
    "next": "15.0.4",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-flow": "^4.1.0",
    "dotenv-flow-cli": "^1.1.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.4",
